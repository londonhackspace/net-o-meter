# Xilinx BGA packages
# data from XAPP157
# Entered by Darrell Harmon
# values for all 1mm pitch Xilinx BGA
	type = "bga"
	pitch = 1mm
	silkwidth = 10 mils
	silkoffset = 1mm		# used if silkbox is invalid
	dia = 0.4mm
	maskclear = 6 mil		# total additional opening: 3 mils from pad to mask
	polyclear = 6 mil
# Xilinx FT256 1mm pitch BGA
	silkboxwidth = 17mm
	silkboxheight = 17mm
	rows = 16
	cols = 16
